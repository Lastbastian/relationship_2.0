<% provide(:title, 'New Questionnaire') %>
<h1>New</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@questionnaire) do |f| %>
      <%= f.fields_for :questions do |builder| %>
        <%= render 'question_fields', :f => builder %>
      <% end %>

      <%= f.submit "Submit", class: "btn btn-primary" %>

    <% end %>
  </div>
</div>

